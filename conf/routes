# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                    controllers.Register.index

# Login Page
GET        /login               controllers.Login.index
POST       /login               controllers.Login.login
# Home page
#POST    /insert/cat                 controllers.Application.insertCat
#POST    /insert/dog                 controllers.Application.insertDog

GET         /test               controllers.Login.test

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file        controllers.Assets.at(path="/public", file)
